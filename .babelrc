{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["IOS >= 8", "Android >= 4.0"]
        },
        "exclude": ["transform-typeof-symbol"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2
      }
    ],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ]
}
